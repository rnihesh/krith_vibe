{
  "project": "sefs-testing",
  "version": "1.0.0",
  "config": {
    "embedding_model": "nomic-embed-text",
    "llm_model": "llama3.2",
    "max_file_size_mb": 50,
    "supported_languages": ["en", "es", "fr", "de"],
    "clustering": {
      "algorithm": "hdbscan",
      "min_cluster_size": 3,
      "min_samples": 2
    }
  },
  "endpoints": [
    { "name": "search", "path": "/api/search", "method": "GET" },
    { "name": "chat", "path": "/api/chat", "method": "POST" },
    { "name": "files", "path": "/api/files", "method": "GET" }
  ]
}
